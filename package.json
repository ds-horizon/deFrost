{
  "name": "@d11/de-frost",
  "version": "1.0.0-beta.1",
  "description": "To detect frozen frame in react native app",
  "source": "./src/index.tsx",
  "main": "./src/index.tsx",
  "module": "./src/index.tsx",
  "bin": {
    "defrost": "./src/scripts/commander.js"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "web",
    "plugins",
    "*.podspec",
    "!src/web",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-frozen-frame-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --ignore-pattern web/bundle.js",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "release": "release-it",
    "create-build": "node src/scripts/createBuild.js",
    "record": "node src/scripts/record.js",
    "web": "webpack serve --mode production --config webpack.config.js",
    "build-web": "webpack --mode production --config webpack.config.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dream11/de-frost"
  },
  "author": "DS Labs <xyz@gmail.com> (https://github.com/sarthak-d11)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dream11/de-frost/issues"
  },
  "homepage": "https://github.com/dream11/de-frost/#readme",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fast-csv": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/papaparse": "5.3.14",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-modal": "3.16.3",
    "babel-loader": "^8.2.5",
    "chart.js": "4.4.3",
    "chartjs-plugin-annotation": "3.0.1",
    "chartjs-plugin-zoom": "2.2.0",
    "commitlint": "^17.0.2",
    "css-loader": "7.1.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "papaparse": "5.4.1",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-modal": "3.16.1",
    "react-native": "0.74.4",
    "react-native-builder-bob": "^0.29.0",
    "release-it": "^15.0.0",
    "style-loader": "4.0.0",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RTNDefrostSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.frozenframe"
    }
  },
  "create-react-native-library": {
    "type": "module-mixed",
    "languages": "kotlin-swift",
    "version": "0.40.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
